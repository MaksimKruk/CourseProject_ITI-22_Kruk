<Window x:Class="Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Background="Lavender" Width="800"  Height="450">
            <Image x:Name="img" Source="https://upload.wikimedia.org/wikipedia/commons/b/b1/Silver_star.png" Canvas.Left="0" Canvas.Top="0" Height="70" Width="70" />
        </Canvas>
    </Grid>
</Window>
